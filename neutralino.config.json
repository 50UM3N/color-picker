{
    "applicationId": "js.neutralino.colorpicker",
    "version": "1.0.0",
    "defaultMode": "window",
    "port": 0,
    "documentRoot": "/resources/",
    "url": "/",
    "enableServer": true,
    "enableNativeAPI": true,
    "modes": {
        "window": {
            "title": "Color Picker",
            "width": 456,
            "height": 389,
            "fullScreen": false,
            "alwaysOnTop": false,
            "icon": "/resources/appIcon.ico",
            "enableInspector": true,
            "borderless": true,
            "maximize": false,
            "hidden": false,
            "resizable": false,
            "exitProcessOnClose": true,
            "nativeAllowList": ["app.*", "storage.*", "window.*"]
        }
    },
    "cli": {
        "binaryName": "colorpicker",
        "resourcesPath": "/resources/",
        "extensionsPath": "/extensions/",
        "clientLibrary": "/resources/neutralino.js",
        "binaryVersion": "4.3.0",
        "clientVersion": "3.2.0"
    }
}